{
    "env": {
        "browser": true,
        "mocha": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": 0,
        "quotes": 0,
        "eol-last": 0,
        "no-return-assign": 0,
        "no-param-reassign": 0,
        "no-console":0
    }
}